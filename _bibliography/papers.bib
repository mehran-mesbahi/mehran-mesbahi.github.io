@inbook{doi:10.2514/6.2024-2416,
bibtex_show={true},
author = {Michael J. Park and Dylan Markovic and Mehran Mesbahi and Sean Phillips},
title = {Extended Abstract for Collaborative Mission Autonomy for Resilient Highly Integrated Networked Operations (CMA-RHINO)},
booktitle = {AIAA SCITECH 2024 Forum},
chapter = {},
pages = {},
doi = {10.2514/6.2024-2416},
URL = {https://arc.aiaa.org/doi/abs/10.2514/6.2024-2416},
eprint = {https://arc.aiaa.org/doi/pdf/10.2514/6.2024-2416},
    abstract = { The standard process for determining and disseminating tasking for surveillance satellite systems can be a slow and lengthy procedure, especially when faced with occasionally intermittent connections between ground-to-satellite and intersatellite communications. For specific applications with dynamic real-time surveillance requirements, such as defense missions with unpredictable adversaries, real-time satellite tasking would be a critical enabler. The purpose of SSCI’s Collaborative Mission Autonomy for Resilient Highly Integrated Network Operations (CMA-RHINO) effort is to study algorithms that can manage and adjust the satellite network autonomously in real-time, with resilience to disruptions that may occur in real-world scenarios. CMA-RHINO analyzes a selected sample of heuristic and auction-based algorithms; posits a decentralized heuristic-supported algorithm to improve satellite network tasking and reduce the need for ground control intervention; and implements this approach in simulation and benchtop hardware, with promising results. }
}

@inbook{doi:10.2514/6.2024-0090,
bibtex_show={true},
author = {Niyousha Rahimi and Mehran Mesbahi},
title = {Data-Guided Regulator for Adaptive Nonlinear Control},
booktitle = {AIAA SCITECH 2024 Forum},
chapter = {},
pages = {},
doi = {10.2514/6.2024-0090},
URL = {https://arc.aiaa.org/doi/abs/10.2514/6.2024-0090},
eprint = {https://arc.aiaa.org/doi/pdf/10.2514/6.2024-0090},
    abstract = { This paper addresses the problem of designing a data-driven feedback controller for complex nonlinear dynamical systems in the presence of time-varying disturbances with unknown dynamics. Such disturbances are modeled as the “unknown” part of the system dynamics. The goal is to achieve finite-time regulation of system states through direct policy updates while also generating informative data that can subsequently be used for data-driven stabilization or system identification. First, we expand upon the notion of “regularizability” and characterize this system characteristic for a linear time-varying representation of the nonlinear system with locally-bounded higher-order terms. “Rapid-regularizability” then gauges the extent by which a system can be regulated in finite time, in contrast to its asymptotic behavior. We then propose the Data-Guided Regulation for Adaptive Nonlinear Control (DG-RAN) algorithm, an online iterative synthesis procedure that utilizes discrete time-series data from a single trajectory for regulating system states and identifying disturbance dynamics. The effectiveness of our approach is demonstrated on a 6-DOF power descent guidance problem in the presence of adverse environmental disturbances. },
    selected      = true
}


@inbook{doi:10.2514/6.2024-0947,
bibtex_show={true},
author = {Aditya Deole and Beniamino Pozzan and Mehran Mesbahi},
title = {MPC-based Estimation-Aware Trajectory Generation for Uncontrolled Satellite Pose Tracking},
booktitle = {AIAA SCITECH 2024 Forum},
chapter = {},
pages = {},
doi = {10.2514/6.2024-0947},
URL = {https://arc.aiaa.org/doi/abs/10.2514/6.2024-0947},
eprint = {https://arc.aiaa.org/doi/pdf/10.2514/6.2024-0947},
    abstract = { We show that observability based measures can effectively be used for evaluating trajectories that facilitate improved state estimation under uncertain measurements in the space environment. In this direction, we examine the problem of tracking an uncontrolled/uncooperative Target satellite by an Ego spacecraft using output measurements from a Machine Learning (ML) based pose estimator. We then represent state dependent uncertainties to model the output measured from the nonlinear perception map. Using an estimate of the current satellite state, we then proceed to propose a Model Predictive Controller (MPC) based design for generating desirable and informative trajectories for the Ego spacecraft. The MPC optimizes a trajectory that performs a tracking task while improving state estimation for auxiliary purposes. We use observability measures on the perception map to justify that the selected trajectory improves Target tracking. },
    selected      = true
}

